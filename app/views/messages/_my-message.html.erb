
<div class="my-message">
  <div class="message-info">
    <p class="my-msg-username"><strong><%= message.user.username %></strong></p>
    <p class="my-msg-time"><%= message.created_at.strftime("%I:%M %p") %></p>
  </div>
  <% if message.content_type == "text" %>
    <%= message.content %>
  <% else %>
  <div class="my-msg-content-media">
    <%= link_to family_documents_path(@family) do %>
      <%= cl_image_tag message.content %>
        <% end %>
  </div>
<!--   <%= cl_image_tag message.content %>
 -->

  <% end %>
</div>

